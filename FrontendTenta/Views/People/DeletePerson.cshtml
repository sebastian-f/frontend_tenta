        <td style="text-align:right;">Email: </td>
        <td>{{Person.email}}  </td>
    </tr>
    <tr>
        <td></td>
        <td>
            <input type="button" value="Ta bort" ng-click="delete()" />
        </td>
    </tr>

</table> 
